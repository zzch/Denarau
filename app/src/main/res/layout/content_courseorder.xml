<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
                 xmlns:tools="http://schemas.android.com/tools"
                 xmlns:app="http://schemas.android.com/apk/res-auto"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:background="#e7e7e7"
                 app:layout_behavior="@string/appbar_scrolling_view_behavior"
                 tools:showIn="@layout/activity_course_order"
                 tools:context="com.zhongchuangtiyu.denarau.Activities.CourseOrderActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout18">

        <ImageView
            android:background="#ff0000"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageView44"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="20dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Large Text"
            android:id="@+id/textView28"
            android:layout_alignTop="@+id/imageView44"
            android:layout_toEndOf="@+id/imageView44"
            android:layout_marginLeft="20dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Medium Text"
            android:id="@+id/textView29"
            android:layout_alignBottom="@+id/imageView44"
            android:layout_alignStart="@+id/textView28"/>
    </RelativeLayout>

    <WebView
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:id="@+id/webView3"
        android:layout_below="@+id/relativeLayout18"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_below="@+id/webView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:id="@+id/linearLayout4">

        <Button
            android:background="@drawable/position_order_btn_selector"
            android:textSize="15sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="今天11111111"
            android:id="@+id/button"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:layout_margin="5dp"/>

        <Button
            android:background="@drawable/position_order_btn_selector"
            android:textSize="15sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="明天"
            android:id="@+id/button2"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:layout_margin="5dp"/>

        <Button
            android:background="@drawable/position_order_btn_selector"
            android:textSize="15sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="后天"
            android:id="@+id/button3"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:layout_margin="5dp"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout4"
        android:layout_centerHorizontal="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1">

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="09:00-10:00"
                android:id="@+id/button4"
                android:layout_weight="1"
                android:layout_margin="5dp"/>

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12:00-13:00"
                android:id="@+id/button5"
                android:layout_weight="1"
                android:layout_margin="5dp"/>

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="15:00-16:00"
                android:id="@+id/button6"
                android:layout_weight="1"
                android:layout_margin="5dp"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1">

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10:00-11:00"
                android:id="@+id/button7"
                android:layout_margin="5dp"
                android:layout_weight="1"/>

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="13:00-14:00"
                android:id="@+id/button8"
                android:layout_margin="5dp"
                android:layout_weight="1"/>

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="16:00-17:00"
                android:id="@+id/button9"
                android:layout_margin="5dp"
                android:layout_weight="1"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1">

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11:00-12:00"
                android:id="@+id/button10"
                android:layout_weight="1"
                android:layout_margin="5dp"/>

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="14:00-15:00"
                android:id="@+id/button11"
                android:layout_weight="1"
                android:layout_margin="5dp"/>

            <Button
                android:background="@drawable/coach_order_btn_selector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="17:00-18:00"
                android:id="@+id/button12"
                android:layout_weight="1"
                android:layout_margin="5dp"/>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>
